<%-include('.././partials/head')%>
        <link href="/user/css/userDetail.css" rel="stylesheet">
    </head>
    <body> 
        <%-include('.././partials/nav')%>
        <main class="contenedor-todo">
            <div class="contenedor-signup">
                <h1 class="signup">My account</h1>
                    <img class="profile" src="/user/img/uploads/<%=session.image%>">
                    <div class="contenedor-datos">
                        <a href="user/edit/<%=session.id%>" class="edit-link"><i class="fas fa-cog edit"></i></a>
                        <div class="contenedor-nombre">
                            <H4>Name</H4>
                            <H3><%=session.name%></H3>
                            <H4>Last Name</H4>
                            <H3><%=session.lastname%></H3>
                        </div>
                        <div class="contenedor-password">
                                <H4>Email</H4>
                                <h3><%=session.email%></h3>
                                <H4>User Type</H4>
                                <h3>Seller</h3>
                        </div>
                    </div>
            </div>
    
        </main>

    </body>
</html>